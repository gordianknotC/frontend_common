{
  "name": "@gdknot/frontend_common",
  "version": "0.8.4",
  "description": "common js util for frontend development",
  "scripts": {
    "build": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "build:watch": "concurrently --kill-others \"tsc --project tsconfig.build.json && tsc-alias -w -p tsconfig.build.json\"",
    "serve": "vue-cli-service serve",
    "test": "jest",
    "test:watch": "clear && jest --watch",
    "---------- DOC ------------": "",
    "doc": "merge-markdown -m merge_markdown.yml && typedoc src/index.ts",
    "serve:doc": "merge-markdown -m merge_markdown.yml && typedoc src/index.ts && serve docs/build",
    "---------- NPM ------------": "",
    "login": "npm login",
    "publish": "npm publish",
    "---------- INSTALL ---------": "",
    "install:node": "nvm install v14.18.0 && nvm alias default v14.18.0",
    "install:storybook": "npx storybook init",
    "config:vscode": "git config --global core.editor 'code --wait'"
  },
  "files": [
    "dist",
    "tsconfig.json"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "notes": {
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "repository": {
      "type": "git",
      "url": "git+https://github.com/gordianknotC/jsUtil.git"
    }
  },
  "dependencies": {
    "merge-anything": "^5.0.4"
  },
  "peerDependencies": {
    "colors": "^1.4.0"
  },
  "devDependencies": {
    "jest-expect-message": "^1.1.3",
    "@knennigtri/merge-markdown": "^1.1.0",
    "@knodes/typedoc-plugin-pages": "^0.23.1",
    "@types/colors": "^1.2.1",
    "@types/debounce": "^1.2.1",
    "@types/jest": "^29.2.0",
    "@types/node": "^16.0.0",
    "@types/numeral": "^2.0.2",
    "@types/ramda": "^0.28.18",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "colors": "^1.4.0",
    "concurrently": "^7.2.1",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "jest-environment-node": "^29.2.2",
    "pinst": "^3.0.0",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "tsc-alias": "^1.6.7",
    "typedoc": "^0.23.0",
    "typedoc-plugin-merge-modules": "^4.0.1",
    "typedoc-plugin-replace-in-comments": "^3.2.0",
    "typescript": "~4.8.4",
    "vue": "3.2.41",
    "vue-router": "^4.1.6"
  },
  "author": "gordianknot",
  "license": "ISC"
}
